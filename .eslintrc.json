{
  "extends": "airbnb",
  "rules": {
    "parserOptions":{
      "ecmaVersion":6,
      "sourceType":"module",
      "ecmaFeatures":{
        "jsx":true,
        "modules":true,
        "experimentalObjectRestSpread":true
      }
    },
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1,
        "ignoredNodes": ["JSXElement"]
      }
    ],
    "no-console":[
      "error",
      {
        "allow":[
          "error"
        ]
      }
    ],
    "max-len": [
      "error",
      {
        "code": 160,
        "ignoreUrls": true,
        "tabWidth": 2
      }
    ],
    "no-unused-vars":[
      "error",
      {
        "vars":"all",
        "args":"none",
        "ignoreRestSiblings":false
      }
    ],
  }
}